<?xml version="1.0" ?>
<sdf version="1.9">
  <model name="catamaran">
  
    <link name="base_link">
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>package://catamaran_description/models/catamaran/mesh/catamaran.dae</uri>
            <!-- Outside = 1.2 -->
            <!--scale>0.72 0.72 0.72</scale-->
            <!-- Inside = 1.1 -->
            <!--<scale>0.866 0.866 0.866</scale>-->
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>
      
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>package://catamaran_description/models/catamaran/mesh/catamaran.dae</uri>
            <!--<scale>0.866 0.866 0.866</scale>-->
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      
      <inertial>
        <mass>180</mass> <!-- kg -->
        <inertia>
          <ixx>120</ixx>
          <iyy>393</iyy>
          <izz>446</izz>
        </inertia>
      </inertial>
    </link>
    
    <!-- Buoyancy plugin
          Fix buoyancy, it sinks -->
    <plugin name="vrx::PolyhedraBuoyancyDrag"
            filename="libPolyhedraBuoyancyDrag.so">
      <fluid_density>1000</fluid_density>
      <fluid_level>0.0</fluid_level>
      <linear_drag>120.0</linear_drag>
      <angular_drag>20.0</angular_drag>
      <buoyancy name="collision_outer">
        <link_name>base_link</link_name>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>6 6 1</size>
          </box>
        </geometry>
      </buoyancy>
      <wavefield>
        <topic>/vrx/wavefield/parameters</topic>
      </wavefield>
    </plugin>

    <enable_wind>true</enable_wind>


    <!-- Attempt to add rear joints and thrusters -->
    <!-- Thruster rear right -->
    <!--link name="thruster_link_rr"-->
      <!--visual name="visual_rr">
        <geometry>
          <mesh filename="package://wamv_description/models/propeller/mesh/propeller.dae"/>
        </geometry>
      </visual-->
      <!--inertial>
        <mass>1</mass>
      </inertial>
    </link>
    <joint name="thruster_joint_rr" type="fixed">
      <parent>base_link</parent>
      <child>thruster_link_rr</child>
      <pose>0.625 -0.71 -0.3 0 0 0</pose>
    </joint-->

    <!-- Left rear thruster -->
    <!--link name="thruster_link_rl"-->
      <!--visual name="visual_rl">
        <geometry>
          <mesh filename="package://wamv_description/models/propeller/mesh/propeller.dae"/>
        </geometry>
      </visual-->
    <!--/link>
    <joint name="thruster_joint_rl" type="fixed">
      <parent>base_link</parent>
      <child>thruster_link_rl</child>
      <pose>-0.625 -0.71 -0.3 0 0 0</pose>
    </joint-->

    <link name="catamaran/left_engine_link">
      <visual name="left_engine_visual">
        <geometry>
          <mesh>
            <uri>package://wamv_description/models/engine/mesh/engine.dae"</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="left_engine_vertical_axis_collision">
        <pose>-0.16 0 -0.24 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.15 0.83</size>
          </box>
        </geometry>
      </collision>
      <collision name="left_engine_rear_end_collision">
        <pose>-0.34 0 0.12 0 0 0</pose>
        <geometry>
          <box>
            <size>0.12 0.15 0.12</size>
          </box>
        </geometry>
      </collision>
      <inertial>
        <mass>15</mass>
        <inertia>
          <ixx>0.889245</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.911125</iyy>
          <iyz>0.0</iyz>
          <izz>0.078125</izz>
        </inertia>
      </inertial>
    </link>
    <link name="catamaran/left_propeller_link">
      <visual name="left_propeller_visual">
        <geometry>
          <mesh>
            <uri>package://wamv_description/models/propeller/mesh/propeller.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="left_propeller_collision">
        <pose>-0.08 0 0 0 1.57 0</pose>
        <geometry>
          <cylinder>
            <length>0.18</length>
            <radius>0.24</radius>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.008545</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.008545</iyy>
          <iyz>0.0</iyz>
          <izz>0.0144</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="catamaran/left_chassis_engine_joint" type="revolute">
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>10</effort>
          <lower>-3.141592653589793</lower>
          <upper>3.141592653589793</upper>
          <velocity>10</velocity>
        </limit>
      </axis>
      <pose>-2.373776 1.027135 0.318237 0.0 0.0 0.0</pose>
      <parent>base_link</parent>
      <child>catamaran/left_engine_link</child>
    </joint>

    <joint name="catamaran/left_engine_propeller_joint" type="continuous">
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <effort>100</effort>
          <velocity>100</velocity>
        </limit>
        <dynamics> 
          <damping>0.05</damping>
          <friction>0.05</friction>
        </dynamics>
      </axis>
      <parent>catamaran/left_engine_link</parent>
      <child>catamaran/left_propeller_link</child>
      <pose>-0.278156 0 -0.509371 0 0 0</pose>
    </joint>


    <!-- Plugin works?, passed first check -->

    <plugin filename="gz-sim-thruster-system" name="gz::sim::systems::Thruster">
      <joint_name>thruster_link_rl</joint_name>
      <thrust_coefficient>0.004422</thrust_coefficient>
      <fluid_density>1000</fluid_density>
      <propeller_diameter>0.2</propeller_diameter>
      <velocity_control>true</velocity_control>
      <!-- max total thrust / 2 -->
      <max_thrust_cmd>2353.53383667818</max_thrust_cmd>
      <namespace>catamaran</namespace>
      <topic>/catamaran/thrusters/rear_left</topic>
      <!-- Not used by Gazebo but used to run the bridge easily -->
      <name>left</name>
    </plugin>

    <plugin filename="gz-sim-thruster-system" name="gz::sim::systems::Thruster">
      <joint_name>thruster_link_rr</joint_name>
      <thrust_coefficient>0.004422</thrust_coefficient>
      <fluid_density>1000</fluid_density>
      <propeller_diameter>0.2</propeller_diameter>
      <velocity_control>true</velocity_control>
      <!-- max total thrust / 2 -->
      <max_thrust_cmd>2353.53383667818</max_thrust_cmd>
      <namespace>catamaran</namespace>
      <topic>/catamaran/thrusters/rear_right</topic>
      <!-- Not used by Gazebo but used to run the bridge easily -->
      <name>left</name>
    </plugin>

  </model>
</sdf>
