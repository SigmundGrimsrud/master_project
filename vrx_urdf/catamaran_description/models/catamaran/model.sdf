    <!-- Thruster right -->
    <plugin name='gz::sim::systems::Thruster' filename='gz-sim-thruster-system'>
      <joint_name>catamaran/right_engine_propeller_joint</joint_name>
      <thrust_coefficient>0.004422</thrust_coefficient>
