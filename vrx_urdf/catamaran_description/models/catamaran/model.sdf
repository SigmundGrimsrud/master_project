<?xml version="1.0" ?>
<sdf version="1.9">
  <model name="catamaran">
  
    <link name="base_link">
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>package://catamaran_description/models/catamaran/mesh/catamaran.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>
      
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>package://catamaran_description/models/catamaran/mesh/catamaran.dae</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      
      <inertial>
        <mass>180</mass> <!-- kg -->
        <inertia>
          <ixx>120</ixx>
          <iyy>393</iyy>
          <izz>446</izz>
        </inertia>
      </inertial>
    </link>
    
    <!-- Buoyancy plugin -->
    <plugin name="vrx::PolyhedraBuoyancyDrag"
            filename="libPolyhedraBuoyancyDrag.so">
      <fluid_density>1000</fluid_density>
      <fluid_level>0.0</fluid_level>
      <linear_drag>120.0</linear_drag>
      <angular_drag>20.0</angular_drag>
      <buoyancy name="collision_outer">
        <link_name>base_link</link_name>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>6 6 1</size>
          </box>
        </geometry>
      </buoyancy>
      <wavefield>
        <topic>/vrx/wavefield/parameters</topic>
      </wavefield>
    </plugin>

    <enable_wind>true</enable_wind>

    <!-- Thrusters left
    **********************
    **********************
    **********************
    **********************
    -->
    <joint name='catamaran/left_chassis_engine_joint' type='revolute'>
      <pose relative_to='base_link'>-2.9 -3.2 -0.2 0 0 1.57</pose>
      <parent>base_link</parent>
      <child>catamaran/left_engine_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-3.1415926535897931</lower>
          <upper>3.1415926535897931</upper>
          <effort>10</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>
    <link name='catamaran/left_engine_link'>
      <pose relative_to='catamaran/left_chassis_engine_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>15</mass>
        <inertia>
          <ixx>0.88924499999999995</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.91112499999999996</iyy>
          <iyz>0</iyz>
          <izz>0.078125</izz>
        </inertia>
      </inertial>
      <collision name='catamaran/left_engine_link_fixed_joint_lump__left_engine_vertical_axis_collision_collision'>
        <pose>-0.16 0 -0.24 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.15 0.83</size>
          </box>
        </geometry>
      </collision>
      <collision name='catamaran/left_engine_link_fixed_joint_lump__left_engine_rear_end_collision_collision_1'>
        <pose>-0.34 0 0.12 0 0 0</pose>
        <geometry>
          <box>
            <size>0.12 0.15 0.12</size>
          </box>
        </geometry>
      </collision>
      <visual name='catamaran/left_engine_link_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://wamv_description/models/engine/mesh/engine.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <joint name='catamaran/left_engine_propeller_joint' type='revolute'>
      <pose relative_to='catamaran/left_engine_link'>-0.278156 0 -0.509371 0 0 0</pose>
      <parent>catamaran/left_engine_link</parent>
      <child>catamaran/left_propeller_link</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <effort>100</effort>
          <velocity>100</velocity>
          <lower>-10000000000000000</lower>
          <upper>10000000000000000</upper>
        </limit>
        <dynamics>
          <damping>0.050000000000000003</damping>
          <friction>0.050000000000000003</friction>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>
    <link name='catamaran/left_propeller_link'>
      <pose relative_to='catamaran/left_engine_propeller_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.0085450000000000005</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0085450000000000005</iyy>
          <iyz>0</iyz>
          <izz>0.0144</izz>
        </inertia>
      </inertial>
      <collision name='catamaran/left_propeller_link_fixed_joint_lump__left_propeller_collision_collision'>
        <pose>-0.08 0 0 0 1.570796326794897 0</pose>
        <geometry>
          <cylinder>
            <length>0.17999999999999999</length>
            <radius>0.23999999999999999</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name='catamaran/left_propeller_link_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://wamv_description/models/propeller/mesh/propeller.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>


    <!-- Thruster left -->
    <plugin name='gz::sim::systems::Thruster' filename='gz-sim-thruster-system'>
      <joint_name>catamaran/left_engine_propeller_joint</joint_name>
      <thrust_coefficient>0.004422</thrust_coefficient>
      <fluid_density>1000</fluid_density>
      <propeller_diameter>0.2</propeller_diameter>
      <velocity_control>true</velocity_control>
      <max_thrust_cmd>2353.53383667818</max_thrust_cmd>
      <namespace>catamaran</namespace>
      <topic>/catamaran/thrusters/rear_left</topic>
      <name>left</name>
    </plugin>
  </model>
</sdf>
