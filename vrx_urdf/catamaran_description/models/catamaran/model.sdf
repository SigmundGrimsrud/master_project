      <child>catamaran/gps_catamaran_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>1000</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
      </axis>
    </joint>
    <link name='catamaran/gps_catamaran_link'>
      <pose relative_to='catamaran/gps_catamaran_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1</mass>
        <inertia>
          <ixx>0.0064580000000000002</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0064580000000000002</iyy>
